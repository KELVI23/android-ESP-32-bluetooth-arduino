    <!-- Activity with no Title -->



    <style parent="AppTheme" name="ThemeOverlay.MyNoTitleActivity">

    <item name="windowActionBar">false</item>

    <item name="windowNoTitle">true</item>

</style>

    <!-- Dark Buttons -->



    <style parent="ThemeOverlay.AppCompat.Dark" name="ThemeOverlay.MyDarkButton">

    <item name="colorButtonNormal">@color/blue_grey_500</item>

    <item name="android:textColor">@android:color/white</item>

</style>


</resources>
